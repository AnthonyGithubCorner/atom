IF START
SET FULTOGGLE
disableActions( box2 )
disableRender( box2 )
CLEAR START
ENDIF

IF !KEYF
CLEAR HOLDINGF
ENDIF

IF KEYF AND !HOLDINGF
SET HOLDINGF
ENDIF


IF HOLDINGF AND FULTOGGLE
CLEAR FULTOGGLE
CLEAR HOLDINGF
SETG EXT_FULLSCREEN
ENDIF

IF HOLDINGF AND !FULTOGGLE
SET FULTOGGLE
CLEAR HOLDINGF
CLEARG EXT_FULLSCREEN
ENDIF