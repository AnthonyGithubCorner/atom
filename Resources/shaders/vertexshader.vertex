#version 330 core
in vec3 vertex; // <vec2 position, vec2 texCoords>
in vec2 UV;
in vec3 color;


out vec2 TexCoords;

uniform vec3 overrideColor;
uniform mat4 model;
uniform mat4 view;
uniform mat4 projection;


void main()
{
    TexCoords = UV;
    gl_Position = projection*model*vec4(vertex, 1.0);
}